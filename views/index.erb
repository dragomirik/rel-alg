<h2>Relational Algebra Interpretor</h2>
<div class="input-container">
  <div class="program-text">
    <h3>
      Program Lines&nbsp;
      <sup><a href="/">Clear</a></sup>
    </h3>
    <form action="/run" method="post">
      <textarea id="program" name="program" rows="15" cols="70"><%= program %></textarea><br/>
      <input type="submit" value="Run">
    </form>
  </div>
  <div class="input-data">
    <h3>
      Input Data&nbsp;
      <sup><a href="/data">Edit</a></sup>
    </h3>
    <pre><%= input_data %></pre>
  </div>
</div>
<div class="output-container">
  <h3>Output Data:</h3>
  <pre><%= output_data %></pre>
</div>
